{
  "name": "@fastlooptech/layout-components",
  "version": "0.0.1",
  "type": "module",
  "access": "public",
  "license": "MIT",
  "private": false,
  "description": "A collection of layout components for React tailored on the needs of Fastloop",
  "author": {
    "name": "Fastloop",
    "email": "info@fastloop.it",
    "url": "https://fastloop.it"
  },
  "homepage": "https://github.com/fastlooptech/fastlab__frontend_ui_layout#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastlooptech/fastlab__frontend_ui_layout"
  },
  "bugs": {
    "url": "https://github.com/fastlooptech/fastlab__frontend_ui_layout/issues",
    "email": "info@fastloop.it"
  },
  "keywords": [
    "react",
    "fastloop",
    "fastlab",
    "layout",
    "components",
    "ui"
  ],
  "file": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": {
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "scripts": {
    "build:package": "vite build",
    "build:types": "api-extractor run --local",
    "build": "./scripts/build.bash",
    "dev": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "test": "cypress run --component --browser chrome",
    "test:open": "cypress open --component --browser chrome"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@microsoft/api-extractor": "^7.34.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "cypress": "^12.7.0",
    "path": "^0.12.7",
    "storybook": "^6.5.16",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.1.0"
  }
}